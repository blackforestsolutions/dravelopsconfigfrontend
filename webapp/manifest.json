{
  "_version": "1.12.0",
  "sap.app": {
    "id": "de.blackforestsolutions.dravelopsconfigfrontend",
    "type": "application",
    "title": "dravelopsconfigfrontend",
    "applicationVersion": {}
  },
  "sap.ui5": {
    "config": {
      "fullWidth": true
    },
    "dependencies": {
      "minUI5Version": "1.75.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.unified": {},
        "sap.f": {},
        "sap.uxap": {}
      }
    },
    "rootView": {
      "viewName": "de.blackforestsolutions.dravelopsconfigfrontend.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "de.blackforestsolutions.dravelopsconfigfrontend.i18n.i18n",
          "supportedLocales": [
            "en",
            "de"
          ],
          "fallbackLocale": "en"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/styles.css"
        }
      ]
    },
    "contentDensities": {
      "compact": true,
      "cozy": false
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "de.blackforestsolutions.dravelopsconfigfrontend.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "appLaunchpad",
          "target": "launchpad"
        },
        {
          "pattern": "configuration/deploymentconfiguration",
          "name": "deploymentconfiguration",
          "target": "deploymentconfiguration"
        }
      ],
      "targets": {
        "launchpad": {
          "viewId": "launchpad",
          "viewName": "Launchpad",
          "viewLevel": 1
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "fade"
        },
        "deploymentconfiguration": {
          "viewId": "deploymentconfiguration",
          "viewName": "launchpad/configurations/DeploymentConfiguration",
          "transition": "fade"
        }
      }
    },
    "flexEnabled": true
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "fullWidth": true,
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  }
}
