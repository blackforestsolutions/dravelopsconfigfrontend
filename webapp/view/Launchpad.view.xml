<mvc:View
        controllerName="de.blackforestsolutions.dravelopsconfigfrontend.controller.Launchpad"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m">
    <mvc:XMLView viewName="de.blackforestsolutions.dravelopsconfigfrontend.view.Logotoolbar"/>
    <Page showHeader="false"
          class="sapUiContentPadding sapUiResponsivePadding--header sapUiResponsivePadding--subHeader sapUiResponsivePadding--content sapUiResponsivePadding--footer">

        <subHeader>
            <OverflowToolbar>
                <Button text="{i18n>anchorbutton-1}"/>
                <Button text="{i18n>anchorbutton-2}"/>
                <Button text="{i18n>anchorbutton-3}"/>
                <Button text="{i18n>anchorbutton-4}"/>
            </OverflowToolbar>
        </subHeader>
        <content>

            <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                         header="Configure-1" press="tileClicked">
                <TileContent>
                    <TileContent>
                        <ImageContent
                                src="sap-icon://developer-settings"
                                description="Icon">
                        </ImageContent>
                    </TileContent>
                </TileContent>
            </GenericTile>
            <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                         header="Configure-2" press="tileClicked">
                <TileContent>
                    <TileContent>
                        <ImageContent
                                src="sap-icon://developer-settings"
                                description="Icon">
                        </ImageContent>
                    </TileContent>
                </TileContent>
            </GenericTile>
            <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                         header="Configure-3" press="tileClicked">
                <TileContent>
                    <TileContent>
                        <ImageContent
                                src="sap-icon://developer-settings"
                                description="Icon">
                        </ImageContent>
                    </TileContent>
                </TileContent>
            </GenericTile>
        </content>

    </Page>
</mvc:View>
