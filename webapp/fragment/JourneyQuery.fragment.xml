<core:FragmentDefinition
        xmlns:core="sap.ui.core"
        xmlns:f="sap.ui.layout.form"
        xmlns="sap.m">

    <VBox class="sapUiSmallMargin">
        <f:Form id="FormEdit480_121202"
                editable="true">

            <f:layout>
                <f:ResponsiveGridLayout
                        labelSpanXL="3"
                        labelSpanL="3"
                        labelSpanM="3"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="4"
                        emptySpanL="4"
                        emptySpanM="4"
                        emptySpanS="0"
                        columnsXL="1"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="true"/>
            </f:layout>
            <f:formContainers>
                <f:FormContainer>
                    <f:formElements>
                        <f:FormElement label="{i18n>name}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/name}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>query}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/query}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>departureLatitude}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/departureLatitude}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>departureLongitude}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/departureLongitude}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>arrivalLatitude}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/arrivalLatitude}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>arrivalLongitude}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/arrivalLongitude}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>dateTime}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/dateTime}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>isArrivalDateTime}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/isArrivalDateTime}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>language}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/variables/language}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>maxResults}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/maxResults}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>departurePlaceholder}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/departurePlaceholder}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>arrivalPlaceholder}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/arrivalPlaceholder}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="{i18n>maxPastDaysInCalendar}">
                            <f:fields>
                                <Input value="{apisettings>/graphql/playground/tabs/JOURNEY_QUERY/maxPastDaysInCalendar}"
                                       required="true"
                                       editable="{configuration>/input/isJourneyQueryInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>
    </VBox>
</core:FragmentDefinition>
