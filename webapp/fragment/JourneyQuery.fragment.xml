<core:FragmentDefinition
        xmlns:core="sap.ui.core"
        xmlns:f="sap.ui.layout.form"
        xmlns="sap.m">

    <VBox class="sapUiSmallMargin">
        <!--                    TODO add ids-->
        <f:Form id="FormEdit480_121202"
                editable="true">

            <f:layout>
                <f:ResponsiveGridLayout
                        labelSpanXL="3"
                        labelSpanL="3"
                        labelSpanM="3"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="4"
                        emptySpanL="4"
                        emptySpanM="4"
                        emptySpanS="0"
                        columnsXL="1"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="true"/>
            </f:layout>
            <f:formContainers>
                <f:FormContainer>
                    <f:formElements>
                        <f:FormElement label="Name">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/name}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="Query">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/query}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="departureLatitude">
                            <f:fields>
                                <MaskInput mask="CCC.C"
                                    placeholderSymbol="_"
                                    value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/departureLatitude}"
                                    required="true"
                                    editable="{configuration>/isInputEnabled}"
                                    type="Number">
                                    <rules>
                                        <MaskInputRule maskFormatSymbol="C" regex="[0-9 --]"/>
                                    </rules>
                                </MaskInput>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="departureLongitude">
                            <f:fields>
                                <MaskInput mask="CCCC.C"
                                    placeholderSymbol="_"
                                    value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/departureLongitude}"
                                    required="true"
                                    editable="{configuration>/isInputEnabled}"
                                    type="Number">
                                    <rules>
                                        <MaskInputRule maskFormatSymbol="C" regex="[0-9 --]"/>
                                    </rules>
                                </MaskInput>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="arrivalLatitude">
                            <f:fields>
                                <MaskInput mask="CCC.C"
                                    placeholderSymbol="_"
                                    value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/arrivalLatitude}"
                                    required="true"
                                    editable="{configuration>/isInputEnabled}"
                                    type="Number">
                                    <rules>
                                        <MaskInputRule maskFormatSymbol="C" regex="[0-9 --]"/>
                                    </rules>
                                </MaskInput>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="arrivalLongitude">
                            <f:fields>
                                <MaskInput mask="CCC.C"
                                    placeholderSymbol="_"
                                    value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/arrivalLongitude}"
                                    required="true"
                                    editable="{configuration>/isInputEnabled}"
                                    type="Number">
                                    <rules>
                                        <MaskInputRule maskFormatSymbol="C" regex="[0-9 --]"/>
                                    </rules>
                                </MaskInput>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="dateTime">
                            <f:fields>
                                <MaskInput mask="CCCC-CC-CCTCC:CC:CC+CC:CC"
                                    placeholderSymbol="_" 
                                    value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/dateTime}"
                                    required="true"
                                    editable="{configuration>/isInputEnabled}">
                                    <rules>
                                        <MaskInputRule maskFormatSymbol="C" regex="[0-9]"/>
                                    </rules>
                                </MaskInput>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="isArrivalDateTime">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/isArrivalDateTime}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="language">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/variables/language}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="maxResults">
                            <f:fields>
                                <MaskInput mask="CC"
                                    placeholderSymbol="_"
                                    value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/maxResults}"
                                    required="true"
                                    editable="{configuration>/isInputEnabled}">
                                     <rules>
                                        <MaskInputRule maskFormatSymbol="C" regex="^([1-9]|[0-1][0-5])$"/>
                                    </rules>
                                </MaskInput>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="departurePlaceholder">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/departurePlaceholder}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="arrivalPlaceholder">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/arrivalPlaceholder}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement label="maxPastDaysInCalendar">
                            <f:fields>
                                <Input value="{apisettings>/calledObject/graphql/playground/tabs/JOURNEY_QUERY/maxPastDaysInCalendar}"
                                       required="true"
                                       editable="{configuration>/isInputEnabled}"/>
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>
    </VBox>
</core:FragmentDefinition>
